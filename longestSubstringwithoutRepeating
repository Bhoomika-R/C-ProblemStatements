class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        characterSet = set()
        l = 0
        answer = 0
        
        for r in range(len(s)):
            while s[r] in characterSet:
                characterSet.remove(s[l])
                l += 1
            characterSet.add(s[r])
            answer = max(answer, r-l+1)
        return answer
